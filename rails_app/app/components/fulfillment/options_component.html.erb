<%= turbo_frame_tag 'options_frame' do %>
  <% if deliverable? %>
    <%= render Fulfillment::DeliverableComponent.new(options_set: options) %>
  <% elsif restricted? %>
    <%= render Fulfillment::UndeliverableComponent.new(options_set: options) %>
  <% else %>
    <%= render Fulfillment::UnavailableComponent.new(options_set: options) %>
  <% end %>
<% end %>
